<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon_Load">
  <commands>
    <command idMso="Refresh" onAction="OnCustomRefresh" />
    <command idMso="RefreshAll" onAction="OnCustomRefreshAll" />
  </commands>
  <ribbon>
    <tabs>
      <tab idMso="TabData">
        <group id="MySqlForExcelGroup" label="MySQL">
          <toggleButton
            id="ShowMySqlForExcelPane"
            label="MySQL for Excel"
            getImage="GetControlImage"
            onAction="OnClickMySqlForExcel"
            getPressed="GetButtonPressed"
            showImage="true"
            size="large"/>
        </group>
      </tab>
    </tabs>
  </ribbon>
</customUI>

